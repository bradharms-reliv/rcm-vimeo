{
  "name": "rcm/vimeo",
  "description": "Display a Vimeo video",
  "license": "BSD-3-Clause",
  "keywords": [
    "reliv",
    "content",
    "manager",
    "plugin",
    "rcm",
    "vimeo"
  ],
  "homepage": "https://github.com/reliv/rcm-vimeo",
  "authors": [
    {
      "name": "James Jervis",
      "email": "jjervis@relivinc.com",
      "homepage": "http://www.reliv.com/"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "rwoverdijk/assetmanager": "1.*",
    "rcm/core": "^1.7.0",
    "rcm/core-js": "^1.0.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "2.*"
  },
  "suggest": {
    "rcm/vimeo-data": "rcm/vimeo-data allows redirecting to actual download links using Vimeo APIs"
  },
  "autoload": {
    "psr-4": {
      "Rcm\\Vimeo\\": "src/"
    }
  }
}
